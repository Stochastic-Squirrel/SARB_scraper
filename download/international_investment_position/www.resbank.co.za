<script type="text/javascript" ="text/javascript">
    function ShowHideBusyDiv(visibility, busyDivId) {
        var div = document.getElementById(busyDivId);

        div.style.visibility = visibility;

        var divLeft = ((1024 / 2) - 50);
        var divTop = 170;

        if (visibility == 'visible') {
            div.style.display = 'block';
            div.style.zIndex = '0';
            div.style.left = divLeft;
            div.style.top = divTop;
            div.style.position = 'absolute';
        }
        else {
            div.style.display = 'none';
            div.style.position = 'relative';
            div.style.zIndex = '1';
            div.style.left = '0';
            div.style.top = '0';
        }
    }

    function Trim(str) {
        while (str.substring(0, 1) == ' ') // check for white spaces from beginning
        {
            str = str.substring(1, str.length);
        }

        while (str.substring(str.length - 1, str.length) == ' ') // check white space from end
        {
            str = str.substring(0, str.length - 1);
        }
        return str;
    }

    if (window.SetHiddenValueFromDll === undefined) {
        window.SetHiddenValueFromDll = function (hiddenid, ddlId) {
